{
    "manifest_version": 2,
    "name": "Browserpass Chrome OS",
    "description": "Browserpass backend for smart cards on Chrome OS",
    "version": "3.0.3",
    "author": "Maxim Baz <browserpass@maximbaz.com>, Steve Gilberd <steve@erayd.net>, Fabian Henneke <fabian@henneke.me>",
    "app": {
        "background": {
            "page": "background.html"
        }
    },
    "permissions": [
        "alwaysOnTopWindows",
        "idle",
        {
            "fileSystem": ["directory", "retainEntries"]
        }
    ],
    "minimum_chrome_version": "63",
    "key": ""
}
